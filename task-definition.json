{
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "nginx/nginx:latest",
      "memory": "256",
      "cpu": "256",
      "essential": true,
      "portMappings": [
        {
          "containerPort": "443",
          "protocol": "tcp"
        }
      ],
      "links": [
        "app"
      ]
    },
    {
      "name": "app",
      "image": "meridth/website:latest",
      "memory": "256",
      "cpu": "256",
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "bridge",
  "placementConstraints": [],
  "family": "application-stack"
}
